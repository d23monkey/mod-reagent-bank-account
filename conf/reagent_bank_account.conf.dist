[worldserver]

########################################
# Reagent Bank Account Config
########################################
#
#    ReagentBankAccount.Enable
#        Description: Enable the reagent bank (account-wide)
#        Default:     0 - Disabled
#                     1 - Enabled
#

ReagentBankAccount.Enable = 1

########################################
#
#    ReagentBankAccount.Announce
#        Description: Enable Announce a message on login for this module
#        Default:     0 - Disabled
#                     1 - Enabled
#

ReagentBankAccount.Announce = 1